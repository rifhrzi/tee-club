    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "clsx": "2.1.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.12.2",
    "jose": "^6.0.11",
    "midtrans-client": "^1.4.2",
    "next": "14.2.15",
